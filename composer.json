{
    "name": "wapmorgan/yii2-inflection",
    "description": "Inflection extension for Yii2. Support for English / Russian languages to inflect words / names / numbers / money / date&time.",
    "keywords": ["yii2", "inflection", "english", "russian"],
    "type": "yii2-extension",
    "license": "GPLv3",
    "autoload": {
        "psr-4": {
            "wapmorgan\\yii2inflection\\": "src/"
        }
    },
    "require": {
        "yiisoft/yii2": "^2.0",
        "wapmorgan/morphos": "~3.2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload-dev": {
        "classmap": ["tests/"]
    }
}
